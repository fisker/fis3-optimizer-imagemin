{
  "name": "fis3-optimizer-imagemin",
  "version": "0.0.0",
  "private": true,
  "description": "a image optimizer plugin of fis3 based on imagemin",
  "keywords": [
    "fis",
    "fis3",
    "fis3-optimizer-imagemin",
    "image",
    "imagemin",
    "optimizer"
  ],
  "homepage": "https://github.com/fisker/fis3-optimizer-imagemin#readme",
  "bugs": {
    "url": "https://github.com/fisker/fis3-optimizer-imagemin/issues"
  },
  "repository": "https://github.com/fisker/fis3-optimizer-imagemin",
  "license": "MIT",
  "author": {
    "name": "fisker Cheung",
    "email": "lionkay@gmail.com",
    "url": "https://www.fiskercheung.com/"
  },
  "sideEffects": false,
  "workspaces": {
    "packages": [
      "packages/*"
    ],
    "nohoist": []
  },
  "scripts": {
    "build": "node -r esm ./scripts/build-packages.js",
    "clean": "run-p clean:*",
    "clean:dist": "del-cli dist",
    "clean:packages": "del-cli packages",
    "dist": "run-p dist:*",
    "dist:npm": "np --yolo --no-yarn",
    "format": "run-p format:*",
    "format:eslint": "yarn lint:eslint --fix",
    "format:markdown": "yarn lint:markdown --fix",
    "format:package-json": "sort-package-json \"package.json\" \"packages/*/package.json\"",
    "format:prettier": "yarn lint:prettier --write",
    "lint": "run-p lint:*",
    "lint:eslint": "eslint \"**/*.{js,mjs,vue}\"",
    "lint:markdown": "markdownlint \"**/*.md\" --ignore \"**/node_modules/**\"",
    "lint:package-json": "yarn run format:package-json --check",
    "lint:prettier": "prettier \"**/*.{css,html,js,json,less,md,scss,ts,vue,yaml,yml}\" --check",
    "release": "run-s lint clean build format && lerna publish"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "dependencies": {
    "imagemin": "7.0.1",
    "imagemin-giflossy": "5.1.10",
    "imagemin-gifsicle": "7.0.0",
    "imagemin-guetzli": "2.0.0",
    "imagemin-jpeg-recompress": "6.0.0",
    "imagemin-jpegoptim": "6.0.0",
    "imagemin-jpegtran": "6.0.0",
    "imagemin-mozjpeg": "8.0.0",
    "imagemin-optipng": "7.1.0",
    "imagemin-pngcrush": "6.0.0",
    "imagemin-pngout": "4.0.0",
    "imagemin-pngquant": "8.0.0",
    "imagemin-svgo": "7.1.0",
    "imagemin-upng": "2.0.2",
    "imagemin-webp": "5.1.0",
    "imagemin-zopfli": "6.0.0",
    "promise-synchronizer": "1.1.9"
  },
  "devDependencies": {
    "@babel/core": "7.8.6",
    "@babel/preset-env": "7.8.6",
    "@commitlint/cli": "8.3.5",
    "@fisker/commitlint-config": "1.1.7",
    "@fisker/eslint-config": "1.6.6",
    "@fisker/husky-config": "1.1.9",
    "@fisker/lint-staged-config": "1.0.12",
    "@fisker/prettier-config": "1.0.29",
    "cz-conventional-changelog": "3.1.0",
    "del-cli": "3.0.0",
    "eslint": "6.8.0",
    "esm": "3.2.25",
    "husky": "4.2.3",
    "json-stable-stringify": "1.0.1",
    "lerna": "3.20.2",
    "lint-staged": "10.0.8",
    "lodash": "4.17.15",
    "markdownlint-cli": "0.22.0",
    "npm-run-all": "4.1.5",
    "prettier": "1.19.1",
    "sort-package-json": "1.40.0",
    "write-prettier-file": "1.2.6"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  }
}
