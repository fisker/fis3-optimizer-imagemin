{
  "name": "fis3-optimizer-imagemin",
  "version": "4.0.2",
  "description": "a image optimizer plugin of fis3 based on imagemin",
  "private": true,
  "repository": "https://github.com/fisker/fis3-plugins",
  "keywords": [
    "fis",
    "fis3",
    "fis3-optimizer-imagemin",
    "image",
    "imagemin",
    "optimizer"
  ],
  "author": {
    "name": "fisker Cheung",
    "email": "lionkay@gmail.com",
    "url": "https://www.fiskercheung.com/"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/fisker/fis3-optimizer-imagemin/issues"
  },
  "readmeFilename": "readme.md",
  "devDependencies": {
    "@babel/core": "7.4.0",
    "@babel/preset-env": "7.4.2",
    "@commitlint/cli": "7.5.2",
    "@commitlint/config-conventional": "7.5.0",
    "cz-conventional-changelog-emoji": "0.1.0",
    "eslint": "5.16.0",
    "eslint-config-fisker": "^4.3.0",
    "esm": "^3.2.22",
    "husky": "^1.3.1",
    "json-stable-stringify": "1.0.1",
    "lint-staged": "^8.1.4",
    "lodash": "4.17.11",
    "markdownlint-cli": "0.14.1",
    "npm-run-all": "4.1.5",
    "prettier": "1.16.4",
    "rimraf": "^2.6.3"
  },
  "dependencies": {
    "imagemin": "^6.1.0",
    "imagemin-giflossy": "^5.1.10",
    "imagemin-gifsicle": "^6.0.1",
    "imagemin-guetzli": "^2.0.0",
    "imagemin-jpeg-recompress": "^6.0.0",
    "imagemin-jpegoptim": "^6.0.0",
    "imagemin-jpegtran": "^6.0.0",
    "imagemin-mozjpeg": "^8.0.0",
    "imagemin-optipng": "^6.0.0",
    "imagemin-pngcrush": "^6.0.0",
    "imagemin-pngout": "^4.0.0",
    "imagemin-pngquant": "^7.0.0",
    "imagemin-svgo": "^7.0.0",
    "imagemin-upng": "^1.2.5",
    "imagemin-webp": "^5.0.0",
    "imagemin-zopfli": "^6.0.0",
    "promise-synchronizer": "^1.0.14"
  },
  "scripts": {
    "build": "node -r esm ./scripts/build-packages.js",
    "clean": "rimraf packages",
    "release": "run-s lint clean build release:*",
    "release:npm": "cd ./scripts && sh ./publish-npm.sh",
    "lint": "npm-run-all lint:*",
    "lint:js": "eslint **/*.js",
    "lint:markdown": "markdownlint {readme.md,packages/**/*.md}"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog-emoji"
    }
  },
  "homepage": "https://github.com/fisker/fis3-optimizer-imagemin#readme"
}
