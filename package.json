{
  "private": true,
  "name": "fis3-optimizer-imagemin",
  "description": "a image optimizer plugin of fis3 based on imagemin",
  "license": "MIT",
  "author": {
    "name": "fisker Cheung",
    "email": "lionkay@gmail.com",
    "url": "https://www.fiskercheung.com/"
  },
  "homepage": "https://github.com/fisker/fis3-optimizer-imagemin#readme",
  "repository": "https://github.com/fisker/fis3-plugins",
  "bugs": {
    "url": "https://github.com/fisker/fis3-optimizer-imagemin/issues"
  },
  "workspaces": {
    "packages": [
      "packages/*"
    ],
    "nohoist": []
  },
  "version": "4.0.6",
  "scripts": {
    "build": "node -r esm ./scripts/build-packages.js",
    "clean": "rimraf packages",
    "format": "run-p format:*",
    "format:eslint": "yarn lint:eslint --fix",
    "format:prettier": "prettier \"**/*.{css,html,js,json,less,md,scss,ts,vue,yaml,yml}\" --write",
    "lint": "run-p lint:*",
    "lint:eslint": "eslint \"**/*.{js,mjs,vue}\"",
    "lint:markdown": "markdownlint \"**/*.md\" --ignore \"**/node_modules/**\"",
    "lint:prettier": "prettier \"**/*.{css,html,js,json,less,md,scss,ts,vue,yaml,yml}\" --check",
    "release": "run-s lint clean build format && lerna publish"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog-emoji"
    }
  },
  "dependencies": {
    "imagemin": "^7.0.0",
    "imagemin-giflossy": "^5.1.10",
    "imagemin-gifsicle": "^6.0.1",
    "imagemin-guetzli": "^2.0.0",
    "imagemin-jpeg-recompress": "^6.0.0",
    "imagemin-jpegoptim": "^6.0.0",
    "imagemin-jpegtran": "^6.0.0",
    "imagemin-mozjpeg": "^8.0.0",
    "imagemin-optipng": "^7.0.0",
    "imagemin-pngcrush": "^6.0.0",
    "imagemin-pngout": "^4.0.0",
    "imagemin-pngquant": "^8.0.0",
    "imagemin-svgo": "^7.0.0",
    "imagemin-upng": "^1.2.8",
    "imagemin-webp": "^5.1.0",
    "imagemin-zopfli": "^6.0.0",
    "promise-synchronizer": "^1.1.1"
  },
  "devDependencies": {
    "@babel/core": "7.5.4",
    "@babel/preset-env": "7.5.4",
    "@commitlint/cli": "8.0.0",
    "@fisker/commitlint-config": "^1.1.0",
    "@fisker/eslint-config": "^1.2.5",
    "@fisker/husky-config": "^1.1.0",
    "@fisker/lint-staged-config": "^1.0.4",
    "@fisker/prettier-config": "^1.0.17",
    "cz-conventional-changelog-emoji": "0.1.0",
    "eslint": "6.0.1",
    "esm": "^3.2.25",
    "husky": "^3.0.0",
    "json-stable-stringify": "1.0.1",
    "lerna": "^3.15.0",
    "lint-staged": "^9.1.0",
    "lodash": "4.17.14",
    "markdownlint-cli": "0.17.0",
    "npm-run-all": "4.1.5",
    "prettier": "1.18.2",
    "rimraf": "^2.6.3",
    "write-prettier-file": "1.2.0"
  },
  "keywords": [
    "fis",
    "fis3",
    "fis3-optimizer-imagemin",
    "image",
    "imagemin",
    "optimizer"
  ],
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "sideEffects": false
}
