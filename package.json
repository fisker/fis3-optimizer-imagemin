{
  "name": "fis3-optimizer-imagemin",
  "version": "0.0.0",
  "private": true,
  "description": "a image optimizer plugin of fis3 based on imagemin",
  "keywords": [
    "fis",
    "fis3",
    "fis3-optimizer-imagemin",
    "image",
    "imagemin",
    "optimizer"
  ],
  "homepage": "https://github.com/fisker/fis3-optimizer-imagemin#readme",
  "bugs": {
    "url": "https://github.com/fisker/fis3-optimizer-imagemin/issues"
  },
  "repository": "fisker/fis3-optimizer-imagemin",
  "funding": "https://github.com/fisker/fis3-optimizer-imagemin?sponsor=1",
  "license": "MIT",
  "author": {
    "name": "fisker Cheung",
    "email": "lionkay@gmail.com",
    "url": "https://www.fiskercheung.com/"
  },
  "sideEffects": false,
  "workspaces": {
    "packages": [
      "packages/*"
    ],
    "nohoist": []
  },
  "scripts": {
    "build": "node -r esm ./scripts/build-packages.js",
    "clean": "run-p clean:*",
    "clean:dist": "del-cli dist",
    "clean:packages": "del-cli packages",
    "dist": "run-p dist:*",
    "dist:npm": "np --yolo --no-yarn",
    "format": "run-p format:*",
    "format:eslint": "yarn lint:eslint --fix",
    "format:markdown": "yarn lint:markdown --fix",
    "format:package-json": "sort-package-json \"package.json\" \"packages/*/package.json\"",
    "format:prettier": "yarn lint:prettier --write",
    "lint": "run-p lint:*",
    "lint:eslint": "eslint \"**/*.{js,mjs,cjs,vue}\"",
    "lint:markdown": "markdownlint \"**/*.md\" --ignore \"**/node_modules/**\"",
    "lint:package-json": "yarn run format:package-json --check",
    "lint:prettier": "prettier \"**/*.{css,html,js,cjs,mjs,json,less,md,scss,ts,vue,yaml,yml}\" --check",
    "release": "run-s lint clean build format && lerna publish"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "dependencies": {
    "imagemin": "7.0.1",
    "imagemin-giflossy": "5.1.10",
    "imagemin-gifsicle": "7.0.0",
    "imagemin-guetzli": "3.0.0",
    "imagemin-jpeg-recompress": "7.0.0",
    "imagemin-jpegoptim": "7.0.0",
    "imagemin-jpegtran": "7.0.0",
    "imagemin-mozjpeg": "9.0.0",
    "imagemin-optipng": "8.0.0",
    "imagemin-pngcrush": "7.0.0",
    "imagemin-pngout": "5.0.0",
    "imagemin-pngquant": "9.0.0",
    "imagemin-svgo": "8.0.0",
    "imagemin-upng": "2.0.2",
    "imagemin-webp": "6.0.0",
    "imagemin-zopfli": "7.0.0",
    "promise-synchronizer": "2.0.0"
  },
  "devDependencies": {
    "@babel/core": "7.10.5",
    "@babel/preset-env": "7.10.4",
    "@commitlint/cli": "9.1.1",
    "@fisker/commitlint-config": "1.3.0",
    "@fisker/eslint-config": "3.0.0",
    "@fisker/husky-config": "3.1.0",
    "@fisker/lint-staged-config": "2.2.0",
    "@fisker/prettier-config": "3.0.0",
    "cz-conventional-changelog": "3.2.0",
    "del-cli": "3.0.1",
    "eslint": "7.5.0",
    "esm": "3.2.25",
    "husky": "4.2.5",
    "json-stable-stringify": "1.0.1",
    "lerna": "3.22.1",
    "lint-staged": "10.2.11",
    "lodash": "4.17.19",
    "markdownlint-cli": "0.23.2",
    "npm-run-all": "4.1.5",
    "prettier": "2.0.5",
    "sort-package-json": "1.44.0",
    "write-prettier-file": "1.2.6"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  }
}
